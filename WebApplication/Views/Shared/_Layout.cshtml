<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title</title>
    
    @Styles.Render("~/bundles/font-awesome/css")
    @Styles.Render("~/bundles/main/css")
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">

</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">
    
    @Html.Partial("_SideBar")

    <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                @Html.Partial("_TopBar")

                <!-- Begin Page Content -->
                @RenderBody()

            </div>
            <!-- End of Main Content -->

        @Html.Partial("_Footer")

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    @Styles.Render("~/bundles/jquery/js")
    @Styles.Render("~/bundles/bootstrap/js")
    @Styles.Render("~/bundles/main/js")
    @Styles.Render("~/bundles/chart-js/js")
    @Styles.Render("~/bundles/demo/js")
    @Styles.Render("~/bundles/font-awesome/js")
</body>

</html>